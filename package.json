  "author": "Mark Percival <m@mdp.im>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.10.3"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "jest": "^27.2.4",
    "openapi-types": "^9.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
