{
  "name": "@flyweight.cloud/swaggerist",
  "version": "0.2.1",
  "description": "Quickly build swagger definitions programatically",
  "homepage": "https://github.com/flyweightcloud/request",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "docs:gen": "npx typedoc src/index.ts",
    "docs:watch": "npx typedoc --watch src/index.ts",
    "test": "jest --config jestconfig.json",
    "package": "npm run lint && npm run test && npm run build && npm publish --access public"
  },
  "author": "Mark Percival <m@mdp.im>",
  "license": "MIT",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "openapi-types": "^12.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.0",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
