{
  "name": "@flyweight.cloud/swaggerist",
  "version": "0.1.1",
  "description": "Quickly build swagger definitions programatically",
  "homepage": "https://github.com/flyweightcloud/request",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest --config jestconfig.json",
    "package": "npm run lint && npm run test && npm run build && npm publish --access public"
